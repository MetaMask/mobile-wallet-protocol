{
	"name": "@metamask/mobile-wallet-protocol-core",
	"version": "0.1.0",
	"private": true,
	"homepage": "https://github.com/MetaMask/mobile-wallet-protocol/tree/main/packages/core#readme",
	"bugs": {
		"url": "https://github.com/MetaMask/mobile-wallet-protocol/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/MetaMask/mobile-wallet-protocol.git"
	},
	"license": "MIT",
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.js"
		},
		"./storage/async-storage": {
			"types": "./dist/storage/async-storage.d.ts",
			"import": "./dist/storage/async-storage.mjs",
			"require": "./dist/storage/async-storage.js"
		},
		"./storage/indexed-db": {
			"types": "./dist/storage/indexed-db.d.ts",
			"import": "./dist/storage/indexed-db.mjs",
			"require": "./dist/storage/indexed-db.js"
		},
		"./storage/in-memory": {
			"types": "./dist/storage/in-memory.d.ts",
			"import": "./dist/storage/in-memory.mjs",
			"require": "./dist/storage/in-memory.js"
		}
	},
	"scripts": {
		"build": "tsup src/index.ts src/storage/*.ts --format cjs,esm --dts"
	},
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/"
	},
	"dependencies": {
		"centrifuge": "^5.3.5",
		"eciesjs": "^0.4.15",
		"eventemitter3": "^5.0.1",
		"uuid": "^11.1.0"
	},
	"devDependencies": {
		"@react-native-async-storage/async-storage": "^1.23.1",
		"@types/uuid": "^10.0.0",
		"@types/ws": "^8.18.1",
		"tsup": "^8.5.0",
		"ws": "^8.18.2"
	},
	"peerDependencies": {
		"@react-native-async-storage/async-storage": ">=1.23.0"
	},
	"peerDependenciesMeta": {
		"@react-native-async-storage/async-storage": {
			"optional": true
		}
	}
}
